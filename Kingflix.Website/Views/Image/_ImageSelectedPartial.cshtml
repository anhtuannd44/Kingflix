@model IEnumerable<Kingflix.Models.Image>

@if (Model.Count() != 0)
{
    foreach (var item in Model)
    {
        <div class="col-md-3 px-0">
            <img src="~/Content/Upload/Images/@item.ImageId" style="width: 100%;" />
        </div>
    }
}
else
{
    <p class="text-danger text-center w-100">Bạn chưa chọn hình nào</p>
}

